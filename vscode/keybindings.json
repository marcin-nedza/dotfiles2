// Umieść swoje powiązania klawiszy w tym pliku, aby zastąpić ustawienia domyślneauto[]
[
  {
    "key": "space+l f",
    "command": "prettier.forceFormatDocument",
    "when":"vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "tab",
    "command": "-tabnine.tab-override",
    "when": "suggestWidgetVisible && tabnine.tab-override && textInputFocus"
  },
  {
    "key": "ctrl+shift+n",
    "command": "-workbench.action.newWindow"
  },
  {
    "key": "ctrl+shift+n",
    "command": "explorer.newFile"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || editorHasCodeActionsProvider "
  },
  {
    "key": "ctrl+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.focus",
    "when": "editorFocus && !suggestWidgetHasFocusedSuggestion && !suggestWidgetVisible && !interactivePlaygroundFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "j",
    "command": "workbench.action.interactivePlayground.arrowDown",
    "when": "interactivePlaygroundFocus && !editorTextFocus"
  },
  {
    "key": "k",
    "command": "workbench.action.interactivePlayground.arrowUp",
    "when": "interactivePlaygroundFocus && !editorTextFocus"
  },

  {
    "key": "space a",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "a",
    "command": "explorer.newFile",
    "when": "explorerViewletFocus && !inputFocus"
},

   { "key": "space e", "command": "workbench.action.toggleSidebarVisibility",
   "when": "editorTextFocus  && vim.active && vim.mode == 'Normal'"
},

{ 
    "key":  "space p f",
    "command": "workbench.action.quickOpen" ,
    "when":"editorTextFocus && vim.active && vim.mode == 'Normal'"
},
{
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenNavigateNext",
    "when": "inQuickOpen"
},
{
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenNavigatePrevious",
    "when": "inQuickOpen"
},
{
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "editorFocus"
},
// {
//     "key": "ctrl+a n",
//     "command": "workbench.action.nextEditor"
// },
// {
//     "key": "ctrl+a p",
//     "command": "workbench.action.previousEditor"
// },
{
    "key": "space c o",
    "command": "editor.action.organizeImports",
    "when":"editorTextFocus && vim.active && vim.mode=='Normal'"
},
{
  "key": "ctrl+a -",
  "command": "workbench.action.terminal.new"
},
{
    "key": "ctrl+a x",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
},
{
  "key": "ctrl+a \\",
  "command": "workbench.action.terminal.split",
  "when": "terminalFocus"
},
{
  "key": "ctrl+a w",
  "command": "workbench.action.closeOtherEditors",
  "when": "editorFocus"
},
{
  "key": "ctrl+shift+j",
  "command": "workbench.action.terminal.focusNext",
  "when": "terminalFocus"
},
{
  "key": "ctrl+shift+k",
  "command": "workbench.action.terminal.focusPrevious",
  "when": "terminalFocus && terminalProcessSupported && !terminalTextSelected"
},{
  "key": "ctrl+l",
  "command": "workbench.action.terminal.focusNextPane",
  "when": "terminalFocus && terminalProcessSupported && !terminalTextSelected"
},{
  "key": "ctrl+h",
  "command": "workbench.action.terminal.focusPreviousPane",
  "when": "terminalFocus && terminalProcessSupported && !terminalTextSelected"
},

{
  "key": "ctrl+j",
  "command": "-workbench.action.togglePanel"
},

]