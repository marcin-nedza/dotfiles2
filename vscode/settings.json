{
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "none",
    "emmet.triggerExpansionOnTab": true,
    "editor.minimap.enabled": false,
    "opensslutils.useWsl": true,
    "opensslutils.opensslPath": "",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "explorer.confirmDragAndDrop": false,
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.renderSideBySide": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.colorTheme": "One Dark Pro Darker",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editor.enablePreview": false,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "update.showReleaseNotes": false,
    "workbench.iconTheme": "material-icon-theme",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.codeLens": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorCustomizations": {},
    "editor.lineNumbers": "relative",
    "keyboard.dispatch": "keyCode",
    "vim.commandLineModeKeyBindings": [],
    "git.openRepositoryInParentFolders": "never",
    "editor.codeLens": false,
    "npm.keybindingsChangedWarningShown": true,
    "errorLens.lintFilePaths": {
        "eslint": [
            "**/*.eslintrc.{js,cjs,yaml,yml,json}",
            "**/*package.json"
        ],
        "Stylelint": [
            "**/*.stylelintrc",
            "**/*.stylelintrc.{cjs,js,json,yaml,yml}",
            "**/*stylelint.config.{cjs,js}",
            "**/*package.json"
        ]
    },
    "errorLens.excludeBySource": [
        "csharp(RCS1036)",
        "csharp(CS1998)"
    ],
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "mssql.objectExplorer.groupBySchema": false,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "k",
                "r"
            ],
            "commands": [
                "cursorRight",
                "<Esc>"
            ],
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<S-h>"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "<S-l>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "workbench.activityBar.location": "default",
    "window.menuBarVisibility": "toggle",
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 12,
    "java.project.outputPath": "/out",
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    }
}